{
  "name": "then-jenkins",
  "description": "Jenkins client",
  "version": "0.7.0",
  "main": "./lib",
  "dependencies": {
    "bluebird": "^2.9.12",
    "jenkins": "^0.7.0"
  },
  "devDependencies": {
    "async": "^0.9.0",
    "istanbul": "^0.3.0",
    "jscs": "^1.5.8",
    "jshint": "^2.5.2",
    "lodash": "^3.2.0",
    "mocha": "^2.0.1",
    "nock": "^0.59.1",
    "node-uuid": "^1.4.1",
    "should": "^5.0.0"
  },
  "keywords": [
    "jenkins"
  ],
  "author": "theverything (Jeffrey Horn)",
  "repository": {
    "type": "git",
    "url": "https://github.com/theverything/then-jenkins.git"
  },
  "bugs": {
    "url": "https://github.com/theverything/then-jenkins/issues"
  },
  "scripts": {
    "cover:display": "npm run cover && open coverage/lcov-report/index.html",
    "cover": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --recursive --check-leaks --timeout 15000",
    "lint": "./node_modules/.bin/jshint lib test",
    "style": "./node_modules/.bin/jscs lib test",
    "_test": "mocha --recursive --check-leaks --timeout 15000",
    "test": "npm run lint && npm run style && ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --recursive --check-leaks --timeout 15000"
  },
  "engines": {
    "node": ">=0.12",
    "npm": "^2.5.0"
  }
}
